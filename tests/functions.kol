func sayHello() {
    print "Hello!";
}

sayHello();

func greet(name) {
    print "Hello, " + name + "!";
}

greet("World");
greet("Koala");

func add(a, b) {
    return a + b;
}

print add(1, 2);
print add(3, 4);

func factorial(n) {
    if (n <= 1) return 1;
    return n * factorial(n - 1);
}

print factorial(5);

func fib(n) {
    if (n <= 1) return n;
    return fib(n - 1) + fib(n - 2);
}

print fib(10);

func max(a, b) {
    if (a > b) return a;
    return b;
}

print max(10, 20);
print max(30, 15);

func min(a, b) {
    if (a < b) return a;
    return b;
}

print min(10, 20);
print min(30, 15);

func noReturn() {
    print "This function doesn't return anything";
}

print noReturn();

func earlyReturn() {
    print "This will print";
    return;
    print "This won't print";
}

earlyReturn(); 